{
    "name": "platzi-overflow",
    "version": "0.0.0",
    "license": "MIT",
    "engines": {
        "node": "8.9.4"
    },
    "scripts": {
        "ng": "ng",
        "start-dev": "concurrently -r \"npm run startser\" \"npm run startclient\"",
        "startser": "NODE_ENV=development DEBUG=platzi-overflow* nodemon server/index.js --exec babel-node",
        "startclient": "ng serve",
        "start": "NODE_ENV=production node server.js",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build": "npm run buildClient && npm run buildServer",
        "buildClient": "ng build --environment=prod",
        "buildServer": "babel server --out-dir dist/server",
        "postinstall": "npm run build"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.3.6",
        "@angular/cdk": "^2.0.0-beta.8",
        "@angular/cli": "1.3.2",
        "@angular/common": "^4.2.4",
        "@angular/compiler": "^4.2.4",
        "@angular/compiler-cli": "^4.2.4",
        "@angular/core": "^4.2.4",
        "@angular/forms": "^4.2.4",
        "@angular/http": "^4.2.4",
        "@angular/material": "^2.0.0-beta.8",
        "@angular/platform-browser": "^4.2.4",
        "@angular/platform-browser-dynamic": "^4.2.4",
        "@angular/router": "^4.2.4",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-stage-0": "^7.0.0",
        "angular2-moment": "^1.9.0",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-async-functions": "^6.22.0",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2016": "^6.24.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "compression": "^1.7.3",
        "concurrently": "^4.0.1",
        "core-js": "^2.4.1",
        "debug": "^4.0.1",
        "express": "^4.17.3",
        "hammerjs": "^2.0.8",
        "jsonwebtoken": "^8.3.0",
        "mongoose": "^5.3.1",
        "mongoose-unique-validator": "^2.0.2",
        "nodemon": "^1.18.4",
        "path": "^0.12.7",
        "rxjs": "^5.4.2",
        "sweet-scroll": "^3.0.1",
        "typescript": "~2.3.3",
        "url-join": "^4.0.0",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/language-service": "^4.2.4",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "~3.1.1",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.3.2"
    }
}